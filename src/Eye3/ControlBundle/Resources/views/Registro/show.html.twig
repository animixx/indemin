{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Datos</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Camion</th>
                <td>{{ entity.camion }}</td>
            </tr>
            <tr>
                <th>Grua</th>
                <td>{{ entity.grua }}</td>
            </tr>
            <tr>
                <th>Inicio</th>
                <td>{{ entity.inicio|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Duracion</th>
                <td>{{ entity.duracion }}</td>
            </tr>
            <tr>
                <th>Hit</th>
                <td>{{ entity.hit }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('historial') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
